        "axios": "^0.21.1",
        "clsx": "^1.1.0",
        "date-fns": "^2.14.0",
        "dotenv": "8.2.0",
        "framer-motion": "^3.1.1",
        "global": "^4.4.0",
        "history": "^5.0.0",
        "http-proxy-middleware": "^1.0.6",
        "identity-obj-proxy": "3.0.0",
        "jest": "24.9.0",
        "jest-environment-jsdom-fourteen": "1.0.1",
        "jest-resolve": "24.9.0",
        "jest-watch-typeahead": "0.4.2",
        "notistack": "^0.9.7",
        "papaparse": "^5.2.0",
        "prop-types": "^15.7.2",
        "qs": "^6.9.4",
        "react": "^16.8.6",
        "react-beautiful-dnd": "^13.0.0",
        "react-compound-slider": "^2.5.0",
        "react-dom": "^16.8.6",
        "react-player": "^1.11.1",
        "react-transition-group": "^4.4.1",
        "smoothscroll-polyfill": "^0.4.4",
        "socket.io-client": "^3.0.4",
        "universal-router": "^9.0.1"
    },
    "scripts": {
        "start": "snowpack dev",
        "build": "tsc -p tsconfig.prod.json && node scripts/build.js",
        "test": "node scripts/test.js --env=jest-environment-jsdom-sixteen",
        "storybook": "start-storybook -h localhost --port 3000 -s public",
        "build-storybook": "build-storybook",
        "lint": "eslint --ext .ts,.tsx,.js,.jsx src --max-warnings 0",
        "docs": "jsdoc -c jsdoc.conf.json",
        "storybook-docs": "yarn storybook --docs",
        "typecheck": "tsc -p tsconfig.prod.json --noEmit",
        "chromatic": "npx chromatic --project-token CHROMATIC_TOKEN --exit-zero-on-changes"
    },
    "alias": {
        "assets": "./src/assets",
        "components": "./src/components",
        "contexts": "./src/contexts",
        "domains": "./src/domains",
        "hooks": "./src/hooks",
        "layout": "./src/layout",
        "pages": "./src/pages",
        "theme": "./src/theme",
        "utils": "./src/utils"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@babel/core": "7.9.0",
        "@babel/preset-react": "^7.12.10",
        "@babel/preset-typescript": "^7.12.7",
        "@snowpack/plugin-babel": "^2.1.6",
        "@snowpack/plugin-dotenv": "^2.0.5",
        "@snowpack/plugin-react-refresh": "^2.4.0",
        "@snowpack/plugin-typescript": "^1.2.1",
        "@types/faker": "^4.1.4",
        "@types/node": "^14.0.13",
        "@types/papaparse": "^5.0.6",
        "@types/qs": "^6.9.5",
        "@types/react": "^16.9.46",
        "@types/react-beautiful-dnd": "^13.0.0",
        "@types/react-dom": "^16.9.8",
        "@types/smoothscroll-polyfill": "^0.3.1",
        "@types/snowpack-env": "^2.3.3",
        "@types/socket.io-client": "^1.4.33",
        "@typescript-eslint/eslint-plugin": "^2.10.0",
        "@typescript-eslint/parser": "^2.10.0",
        "babel-plugin-import": "^1.13.3",
        "better-docs": "^2.3.0",
        "chromatic": "^5.6.0",
        "eslint": "^6.6.0",
        "eslint-config-airbnb-typescript": "^8.0.2",
        "eslint-config-prettier": "^6.4.0",
        "eslint-plugin-import": "^2.18.2",
        "eslint-plugin-jest": "^23.13.1",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-react": "^7.16.0",
        "eslint-plugin-react-hooks": "^4.0.8",
        "events": "^3.2.0",
        "faker": "https://github.com/bustle/faker.js#master",
        "jsdoc": "^3.6.5",
        "jsdoc-mermaid": "^1.0.0",
        "msw": "^0.21.3",
        "prytaneum-typings": "https://github.com/c2c-project/prytaneum-typings#v1.0.0-alpha.82",
        "snowpack": "^3.0.11",
        "typescript": "^3.9.5"
    },
    "jest": {
        "roots": [
            "<rootDir>/src"
        ],
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,ts,tsx}",
            "!src/**/*.d.ts"
        ],
        "setupFiles": [
            "react-app-polyfill/jsdom"
        ],
        "setupFilesAfterEnv": [
            "<rootDir>/src/setupTests.ts"
        ],
        "testMatch": [
            "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
            "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
        ],
        "testEnvironment": "jest-environment-jsdom-fourteen",
        "transform": {
            "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
            "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
            "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
        },
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
            "^.+\\.module\\.(css|sass|scss)$"
        ],
        "modulePaths": [
            "/home/david/Documents/learn/prytaneum-cra-v4/src"
        ],
        "moduleNameMapper": {
            "^react-native$": "react-native-web",
            "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
        },
        "moduleFileExtensions": [
            "web.js",
            "js",
            "web.ts",
            "ts",
            "web.tsx",
            "tsx",
            "json",
            "web.jsx",
            "jsx",
            "node"
        ],
        "watchPlugins": [
            "jest-watch-typeahead/filename",
            "jest-watch-typeahead/testname"
        ]
    },
    "eslintConfig": {
        "extends": "react-app"
    }
}
